= render :partial => 'spree/admin/shared/product_sub_menu'
- # Include template for autocomplete fields
= render :partial => "spree/admin/variants/autocomplete", :formats => [:js]

- content_for :page_title do
  = t(:editing_consultation)
  %i.icon-arrow-right
  = @consultation.name

- content_for :page_actions do
  %li
    = button_link_to t(:back_to_suppliers_list), spree.admin_suppliers_url, :icon => 'icon-arrow-left'

= render :partial => 'spree/shared/error_messages', :locals => { :target => @consultation }

= form_for [:admin, @consultation] do |f|
  = render :partial => 'form', :locals => { :f => f }
  = render :partial => 'spree/admin/shared/edit_resource_links'

%div{style: 'margin-bottom: 10em'}